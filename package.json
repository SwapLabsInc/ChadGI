{
  "name": "chadgi",
  "version": "1.0.5",
  "description": "ChadGI - Autonomous Task Worker powered by Claude Code. Chad does what Chad wants.",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "chadgi": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
